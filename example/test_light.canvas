        "extDeps" : {},
        "code" : "dfgEntry {
  // create a unique key
  String uidName = handle.uid() + '_' + mesh.uid();

  InlineDrawing draw = handle.getDrawing();
  
  // here you want to create your own custom shader - instead of the 
  // OGL Surface shader pass a constructed shader of your own, for example RTLinesShader
  InlineShader shader = draw.registerShader(OGLSurfaceShader());

  // create a material for our shader  
  InlineMaterial mat = shader.getOrCreateMaterial(uidName);
  // here you can set the uniforms you have defined in your shader
  mat.setUniform('u_diffuseColor', color);
  mat.setUniform('u_ambientColor', ambient);
  mat.setUniform('u_specularColor', specColor);
  mat.setUniform('u_specularFactor', specNum);
  
  //light
  mat.setUniform('u_light0Position', lightdirection);
  mat.setUniform('u_light0Color', lightColor);
  
  
  // create a shape for our mesh
  InlineMeshShape shape = draw.getShape(uidName);
  if(!shape) 
    shape = draw.registerShape(InlineMeshShape(uidName, mesh));
  else
    shape.setMesh(mesh);
  
  // try to see if we have a transform already for this key
  StaticInlineTransform xform = handle.rootTransform.getChild(uidName);
  if(xform == null)
  {
    // create the missing transform
    xform = StaticInlineTransform(uidName,xfo);
    handle.rootTransform.addChild(xform);
    // if the shape associated to an instance is not registred in InlineDrawing (draw.hasShape(name) == false)
    // the instance owns both its material and shape. When such an instance is deleted, 
    // its material and shape are unregisted from InlineDrawing and destroyed when the DrawingHandle is cleared.
    SimpleInlineInstance(uidName, xform, shape, handle.visibility, mat);
}
}"
